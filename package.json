{
  "name": "frontend",
  "private": true,
  "engines": {
    "node": ">=24.11"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "build:export": "NEXT_EXPORT_BUILD=true next build --no-lint",
    "build:standalone": "NEXT_STANDALONE_BUILD=true next build --no-lint",
    "start": "next start",
    "lint:target": "next lint --ignore-path .gitignore --max-warnings 0",
    "lint": "pnpm lint:target --dir .",
    "lint:all": "pnpm lint",
    "lint:fix": "pnpm lint:all --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx'",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}'",
    "generate": "plop",
    "prepare": "next telemetry disable",
    "pre-push-check": "pnpm lint && pnpm test && pnpm build",
    "pre-commit-check": "pnpm typecheck && lint-staged && pnpm knip",
    "release": "release-it --config config/release.config.js --only-version",
    "knip": "knip --config config/knip.config.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.11.0",
    "@tanstack/react-query": "^5.67.0",
    "axios": "^0.27.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "6.0.0",
    "echarts-for-react": "^3.0.5",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "lucide-react": "^0.552.0",
    "next": "15.4.1",
    "next-secure-headers": "^2.2.0",
    "next-themes": "^0.4.6",
    "notistack": "^3.0.2",
    "query-string": "^7.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^9.2.0",
    "sharp": "^0.33.4",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.6",
    "zod": "^3.25.76"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@nimbleways/eslint-config": "^0.3.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.5.14",
    "@types/node": "18.11.18",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-redux": "^7.1.25",
    "eslint": "^8.40.0",
    "eslint-config-next": "15.4.1",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.45.0",
    "lint-staged": "^15.4.3",
    "next-http-proxy-middleware": "^1.2.5",
    "plop": "^3.1.1",
    "release-it": "^18.1.2",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.3"
  },
  "resolutions": {
    "@typescript-eslint/parser": "6.2.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}